  "dependencies": {
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "ejs": "^2.5.7",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "express-enrouten": "^1.3.0",
    "multer": "^1.3.0",
    "nconf": "^0.10.0",
    "swagger-jsdoc": "^1.9.7",
