    "eslint-watch": "^3.1.3",
    "jsdom": "^11.5.1",
    "localtunnel": "^1.8.3",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "supertest": "^3.0.0"
